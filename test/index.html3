function celsiusiusToFarenheit(celsius)   
{  
   
  var fahrenheit = celsius * 1.8 + 32;  
   
    return(fahrenheit);  
}  
 

function fahrenheitToCelsius(fahrenheit)   
{  
   
 var celsius = (fahrenheit - 32) * 1.8;  
 
 return (celsius);
     
}   

function calculate(){

var myValue = document.getElementById("my-value");
var celsiusVar = myValue.value;
var fahrenheitVar = celsiusiusToFarenheit(celsiusVar);
var result = document.getElementById("result");
result.innerHTML = fahrenheitVar;

}
var myButton = document.getElementById("my-button");
myButton.onclick = calculate;
